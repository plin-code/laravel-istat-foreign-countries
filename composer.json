{
    "name": "plin-code/laravel-istat-foreign-countries",
    "description": "Laravel package for importing and managing foreign countries data from ISTAT",
    "keywords": [
      "PlinCode",
      "plin-code",
        "laravel",
        "laravel-istat-foreign-countries",
        "istat",
        "foreign-countries",
        "geography"
    ],
    "homepage": "https://github.com/plin-code/laravel-istat-foreign-countries",
    "license": "MIT",
    "authors": [
      {
        "name": "Daniele Barbaro",
        "email": "barbaro.daniele@gmail.com",
        "role": "Developer"
      }
    ],
    "require": {
        "php": "^8.3",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^12.0",
        "league/csv": "^9.24",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "larastan/larastan": "^2.9||^3.0",
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "PlinCode\\IstatForeignCountries\\": "src/",
            "PlinCode\\IstatForeignCountries\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PlinCode\\IstatForeignCountries\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/",
            "Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-laravel-istat-foreign-countries --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PlinCode\\IstatForeignCountries\\IstatForeignCountriesServiceProvider"
            ],
            "aliases": {
                "IstatForeignCountries": "PlinCode\\IstatForeignCountries\\Facades\\IstatForeignCountries"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
